import * as React from 'react';
{%- for entity in Params.Entities -%}
	{%- if entity.HideInMenu == false -%}
import {{entity.Name}}List from "../screens/Entities/{{entity.Name}}/List/{{entity.Name}}List";
import {{entity.Name}}Form from "../screens/Entities/{{entity.Name}}/Form/{{entity.Name}}Form"; 
	{%- endif -%}
{%- endfor -%}

interface ILabelInfo {
    component : React.ComponentType<any>,
    path : string,
	exact: boolean
}

const routes : ILabelInfo[] = 
[
    {%- for entity in Params.Entities -%}
		{%- if entity.HideInMenu == false -%}
    {component: {{entity.Name}}List, path: "{{entity.PluralName | DeCapitalize }}", exact: true},
    {component: {{entity.Name}}Form, path: "{{entity.PluralName | DeCapitalize }}/:id", exact: false},
		{%- endif -%}
	{%- endfor -%}
]

export default routes;